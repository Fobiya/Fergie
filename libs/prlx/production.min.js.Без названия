 ;(function() {
     
     // support transitions
     var support = Modernizr.csstransition;
     // transition end event name
    //  transEndEventNames = {
    //      'WebkitTransition': 'webkitTransitionEnd',
    //      'MozTransition': 'transitionend',
    //      'OTransition': 'oTransitionEnd',
    //      'msTransition': 'MSTransitionEnd',
    //      'transition': 'transitionend'
    //  },
    //  transEndEventName = transEndEventNames[ Modernizr.prefixed( 'transition' ) ],
    //  clickevent = mobilecheck() ? 'touchstart' : 'click';
     
     function init() {
         var inputs = document.getElementsByClassName( 'inputblock' );
         for (i = 0; i < inputs.length; i++) {
             var inputThingy = inputs[i].getElementsByTagName( 'input' )[0];
             inputThingy.addEventListener( 'input', function( ev ) {
                 ev.stopPropagation()
                 ev.preventDefault()
                if(ev.srcElement.value == "" || ev.srcElement.value == null) {
                    // it's empty
                    classie.remove(ev.srcElement.parentElement, 'filled')
                } else {
                    // it's not empty
                    classie.add(ev.srcElement.parentElement, 'filled')
                }
            });
         }
     }
    init()
})()
