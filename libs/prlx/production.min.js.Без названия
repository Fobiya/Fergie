;(function() {
     
     // scrollY function
     function scrollY() {
         return window.pageYOffset || docElem.scrollTop
     }
     
     // also lifted
     // from http://stackoverflow.com/a/11381730/989439
 	function mobilecheck() {
 		var check = false;
 		(function(a){if(/(android|ipad|playbook|silk|bb\d+|meego).+mobile|avantgo|bada\/|blackberry|blazer|compal|elaine|fennec|hiptop|iemobile|ip(hone|od)|iris|kindle|lge |maemo|midp|mmp|netfront|opera m(ob|in)i|palm( os)?|phone|p(ixi|re)\/|plucker|pocket|psp|series(4|6)0|symbian|treo|up\.(browser|link)|vodafone|wap|windows (ce|phone)|xda|xiino/i.test(a)||/1207|6310|6590|3gso|4thp|50[1-6]i|770s|802s|a wa|abac|ac(er|oo|s\-)|ai(ko|rn)|al(av|ca|co)|amoi|an(ex|ny|yw)|aptu|ar(ch|go)|as(te|us)|attw|au(di|\-m|r |s )|avan|be(ck|ll|nq)|bi(lb|rd)|bl(ac|az)|br(e|v)w|bumb|bw\-(n|u)|c55\/|capi|ccwa|cdm\-|cell|chtm|cldc|cmd\-|co(mp|nd)|craw|da(it|ll|ng)|dbte|dc\-s|devi|dica|dmob|do(c|p)o|ds(12|\-d)|el(49|ai)|em(l2|ul)|er(ic|k0)|esl8|ez([4-7]0|os|wa|ze)|fetc|fly(\-|_)|g1 u|g560|gene|gf\-5|g\-mo|go(\.w|od)|gr(ad|un)|haie|hcit|hd\-(m|p|t)|hei\-|hi(pt|ta)|hp( i|ip)|hs\-c|ht(c(\-| |_|a|g|p|s|t)|tp)|hu(aw|tc)|i\-(20|go|ma)|i230|iac( |\-|\/)|ibro|idea|ig01|ikom|im1k|inno|ipaq|iris|ja(t|v)a|jbro|jemu|jigs|kddi|keji|kgt( |\/)|klon|kpt |kwc\-|kyo(c|k)|le(no|xi)|lg( g|\/(k|l|u)|50|54|\-[a-w])|libw|lynx|m1\-w|m3ga|m50\/|ma(te|ui|xo)|mc(01|21|ca)|m\-cr|me(rc|ri)|mi(o8|oa|ts)|mmef|mo(01|02|bi|de|do|t(\-| |o|v)|zz)|mt(50|p1|v )|mwbp|mywa|n10[0-2]|n20[2-3]|n30(0|2)|n50(0|2|5)|n7(0(0|1)|10)|ne((c|m)\-|on|tf|wf|wg|wt)|nok(6|i)|nzph|o2im|op(ti|wv)|oran|owg1|p800|pan(a|d|t)|pdxg|pg(13|\-([1-8]|c))|phil|pire|pl(ay|uc)|pn\-2|po(ck|rt|se)|prox|psio|pt\-g|qa\-a|qc(07|12|21|32|60|\-[2-7]|i\-)|qtek|r380|r600|raks|rim9|ro(ve|zo)|s55\/|sa(ge|ma|mm|ms|ny|va)|sc(01|h\-|oo|p\-)|sdk\/|se(c(\-|0|1)|47|mc|nd|ri)|sgh\-|shar|sie(\-|m)|sk\-0|sl(45|id)|sm(al|ar|b3|it|t5)|so(ft|ny)|sp(01|h\-|v\-|v )|sy(01|mb)|t2(18|50)|t6(00|10|18)|ta(gt|lk)|tcl\-|tdg\-|tel(i|m)|tim\-|t\-mo|to(pl|sh)|ts(70|m\-|m3|m5)|tx\-9|up(\.b|g1|si)|utst|v400|v750|veri|vi(rg|te)|vk(40|5[0-3]|\-v)|vm40|voda|vulc|vx(52|53|60|61|70|80|81|83|85|98)|w3c(\-| )|webc|whit|wi(g |nc|nw)|wmlb|wonu|x700|yas\-|your|zeto|zte\-/i.test(a.substr(0,4)))check = true})(navigator.userAgent||navigator.vendor||window.opera);
 		return check;
 	}
    
    var docElem = window.document.documentElement,
	
    // support transitions
    support = Modernizr.csstransition,
    // transition end event name
    transEndEventNames = {
        'WebkitTransition': 'webkitTransitionEnd',
        'MozTransition': 'transitionend',
        'OTransition': 'oTransitionEnd',
        'msTransition': 'MSTransitionEnd',
        'transition': 'transitionend'
    },
    transEndEventName = transEndEventNames[ Modernizr.prefixed( 'transition' ) ],
    docscroll = 0,
    // click event (if movile use touchstart)
    clickevent = mobilecheck() ? 'touchstart' : 'click';

    function init() {
        var showMenu = document.getElementById( 'hamburger' ),
        perspectiveWrapper = document.getElementById( 'perspective' ),
        container = perspectiveWrapper.querySelector( '.container' ),
        closeButton = document.getElementById( 'close' ),
        header = document.getElementById( 'header' ),
        contentWrapper = container.querySelector( '.wrapper' ),
        menuitems = document.getElementsByClassName( 'menu-toggle' );
        
		var restartParallax;
		var closeRollOverTimeout;
		var animateTimeout;
		
		showMenuFunc = function( ev ) {
			
            ev.stopPropagation()
            ev.preventDefault()
            
            disableScroll();
            classie.add(perspectiveWrapper, 'mutated');
            classie.add(perspectiveWrapper, 'animate' )
            
            // parallaxController.pauseTween();
		}


		
		// function hideHamburger() {
        // 
		// 	closeButton.style.transform = "translate3d(0px, 0px, 0px)";
		// 	showMenu.style.transform = "scale3d(0, 0, 0) translate3d(0px, 200px, 0px)";
        //     closeButton.style.webkitTransform = "translate3d(0px, 0px, 0px)";
        //     showMenu.style.webkitTransform = "scale3d(0, 0, 0) translate3d(0px, 200px, 0px)";
		// 	
		// }
		// 
		// function showHamburger() {
		// 	
        //     showMenu.style.transform = "scale3d(1, 1, 1) translate3d(0px, 0px, 0px)";
		// 	showMenu.style.webkitTransform = "scale3d(1, 1, 1) translate3d(0px, 0px, 0px)";
		// 	
		// }
		// 
		// function closeMenuRollover() {
		// 	
        //     closeButton.style.transform = "rotateZ(90deg) scale(1.1, 1.1)";
		// 	closeButton.style.webkitTransform = "rotateZ(90deg) scale(1.1, 1.1)";
		// 	
		// }
        // 
		// function closeMenuRollout() {
		// 	
        //     closeButton.style.transform = "rotateZ(0) scale(0.85, 0.85)";
		// 	closeButton.style.webkitTransform = "rotateZ(0) scale(0.85, 0.85)";
        // 
		// }
        
        var closeMenu = function(ev){
			
            enableScroll();
			
			
            if( classie.has( perspectiveWrapper, 'animate') ) {
                
				var onEndTransFn = function( ev ) {
					if( support && ( ev.target.className !== 'container' || ev.propertyName.indexOf( 'transform' ) == -1 ) ) return;
					this.removeEventListener( transEndEventName, onEndTransFn );
					

					
				};
				if( support ) {
					perspectiveWrapper.addEventListener( transEndEventName, onEndTransFn );
				}
				classie.remove( perspectiveWrapper, 'animate' );
                classie.remove(perspectiveWrapper, 'mutated');

			}
        }
		
		showMenu.addEventListener( clickevent, showMenuFunc );
        container.addEventListener( clickevent, closeMenu);
        closeButton.addEventListener( clickevent, closeMenu);
        header.addEventListener( clickevent, closeMenu);
        
        perspectiveWrapper.addEventListener( clickevent, function(ev){
            return false
        })
    }
        
    init()
 })()

// controls offsets for the speaker section

;(function() {

	// gather speaker images
	var getImgs 		= document.getElementsByClassName( 'speaker-image' );

	// gather other needed refs
	var letterLeft 		= document.getElementById( 'supertextLeft' );
	var letterRight 	= document.getElementById( 'supertextRight' );
	var reticle			= document.getElementById( 'speakerReticle');
	var locationMovie	= document.getElementById( 'locationMovie' );
	var sponColOne		= document.getElementById( 'sponsorColumn1' );
	var sponColTwo		= document.getElementById( 'sponsorColumn2' );
	var sponColThree	= document.getElementById( 'sponsorColumn3' );


	// to store refs for speaker images and offsets
	var speakerImageObj = [];

	function init() {

		placeSpeakerImages();

		letterLeft.style.top = "3340px";
		letterRight.style.top = "7000px";
		reticle.style.marginTop = 2800 + "px";

		sponColOne.style.top = sponColTwo.style.top = sponColThree.style.top = "500px";

		// locationMovie.style.top = "4000px";

		window.addEventListener('resize', handleResize);

		handleResize(window);
	}

	function placeSpeakerImages() {

		var mar = 800;

		for(i=0; i<getImgs.length; i++) {

			var speaker = getImgs[i];
			speaker.style.marginTop = mar + "px";

			// mar -= 15;

			mar += (10 * i) + 150;

			speakerImageObj.push( {e: speaker, offset: speaker.offsetTop} );

		}
	}

	function updateImages(inc, minus) {

		// var newBase = base + inc;

		var inc = inc;

		for(i=0; i<getImgs.length; i++) {

			var speaker = getImgs[i];
			speaker.style.marginTop = inc + "px";

			inc += minus;	

		}
	}

	function handleResize(ev) {

		var w = window.innerWidth;

		// if(w > 1599) {

		// }
		// if(w < 1600 && w > 1400) {
			// letterLeft.style.marginTop = 100 + "px";
			// letterRight.style.marginTop = 200 + "px";

		// }

		if(w > 1800) {

			reticle.style.marginTop = 2600 + "px";
			letterLeft.style.top = "3100px";
			// locationMovie.style.marginTop = "3800px";
			locationMovie.style.marginTop = "4400px";

		}

		if(w > 1600 && w < 1800) {

			reticle.style.marginTop = 2600 + "px";
			letterLeft.style.top = "3200px";
			locationMovie.style.marginTop = "4400px";

		}

		if(w > 1399 && w < 1600) {

			reticle.style.marginTop = 2600 + "px";
			letterLeft.style.top = "3300px";
			locationMovie.style.marginTop = "4200px";

		}

		if(w < 1400 && w > 1300) {

			reticle.style.marginTop = 2600 + "px";
			locationMovie.style.marginTop = "4200px";
			letterLeft.style.top = "3420px";
			letterRight.style.top = "5900px";
		}

		if(w < 1300 && w > 1200) {

			reticle.style.marginTop = 2800 + "px";
			letterLeft.style.top = "3420px";
			letterRight.style.top = "5400px";
			locationMovie.style.marginTop = "4250px";
			updateImages(850, 175);

		}

		if(w < 1200 && w > 1100) {
			
			locationMovie.style.marginTop = "4000px";
			letterLeft.style.top = "3100px";
			letterRight.style.top = "5000px";
			updateImages(850, 170);

		}

		if(w < 1100 && w > 900) {

			locationMovie.style.marginTop = "3900px";
			letterLeft.style.top = "3000px";
			letterRight.style.top = "5000px";
			updateImages(850, 170);


		}


		if(w < 900 && w > 650) {

			locationMovie.style.marginTop = "3600px";
			letterLeft.style.top = "2800px";
			letterRight.style.top = "5000px";

			updateImages(840, 140);

		}


		if(w < 650 && w > 420) {

			locationMovie.style.top = "2400px";
			reticle.style.marginBottom = "-1300px";
			letterLeft.style.top = "2590px";
			letterRight.style.top = "4420px";
			updateImages(960, 120);

		} 

		// iphone 6+
		if(w < 420 && w > 380) {
			// updateImages(880, 100);
			updateImages(950, 100);
			letterLeft.style.top = "2660px";
			letterRight.style.top = "4150px";
			locationMovie.style.top = "1900px";
		}

		// iphone 6
		if(w < 380 && w > 340) {
			updateImages(950, 100);
			letterLeft.style.top = "2710px";
			letterRight.style.top = "4150px";
			locationMovie.style.top = "2000px";
		}

		// iphone 5 & 4 width
		if(w < 340) {
			updateImages(920, 100);
			letterLeft.style.top = "2750px";
			letterRight.style.top = "4150px";
			locationMovie.style.top = "1900px";
		}


	}


	init();


 })()
/**
 * parallaxy.js 0.0.2
 */
var parallaxController;

;(function() {
	
	parallaxController = this;
	
    // Totally lifted from this: http://stackoverflow.com/questions/11197247/javascript-equivalent-of-jquerys-extend-method
    function extend(){
        for(var i=1; i<arguments.length; i++)
            for(var key in arguments[i])
                if(arguments[i].hasOwnProperty(key))
                    arguments[0][key] = arguments[i][key];
        return arguments[0];
    }
    
    // Lifted from: http://stackoverflow.com/questions/18359093/how-to-copy-javascript-object-to-new-variable-not-by-reference
    // I can't believe that I had to look this up.
    // Object.prototype.copy = function() {
    //     return JSON.parse(JSON.stringify(this));
    // }
    
	var menuClosed = true;
	
    // HTML Stuff
    this.paralaxxed = document.getElementsByClassName("parallaxy-animate");
    
    // Create the logic
    parallaxate = function(parallax_elements, defaultSettings) {
        var elements = parallax_elements; 

        for (i = 0; i < elements.length; i++) {
			
			// console.log(elements[i]);
			
            var settings = JSON.parse(JSON.stringify(defaultSettings)); // defaultSettings.copy();
            var options = JSON.parse(elements[i].getAttribute('parallaxy-options'));
            elements[i].settings = extend(settings, options);
            
            if (options.positionType == 'relative') {
                elements[i].initial_offset = parseInt(window.getComputedStyle(elements[i], null).getPropertyValue('margin-top'), 10);
                elements[i].dataset.positionType = "relative";
            } else {
                elements[i].initial_offset = elements[i].offsetTop;
                elements[i].dataset.positionType = "absolute";
            }
            
            elements[i].dataset = options.direction;
            elements[i].dataset.currentDelta = 0;
            elements[i].dataset.newDelta = 0;
			
            // container.addEventListener("scroll", function(event) {
//                 for (i = 0; i < elements.length; i++) {
//                     var newDelta = (0 - (container.scrollTop * elements[i].settings.multiplier));
//                     if(elements[i].settings.direction == "down") {
//                         newDelta = (0 + (container.scrollTop * elements[i].settings.multiplier));
//                     }
//                     elements[i].dataset.newDelta = newDelta;
//                 }
// 				parallaxController.scrollHandler()
//             });
        }
    }
    
    scrollHandler = function() {

        var that = this;
		var scrollTop = window.pageYOffset;
		
        for (i = 0; i < paralaxxed.length; i++) {
            
            var currentDelta = paralaxxed[i].dataset.currentDelta;
            // var newDelta = paralaxxed[i].dataset.newDelta;
			
			var newDelta = (0 - (scrollTop * paralaxxed[i].settings.multiplier));
            
			if(paralaxxed[i].settings.direction == "down") {
				newDelta = (0 + (scrollTop * paralaxxed[i].settings.multiplier));
			}
			
            // figure out the tween.
            // var tweenDelta = (currentDelta - ((currentDelta - newDelta) * 0.08)); // up
            if(paralaxxed[i].settings.direction == "down") {
            
            } else {
				
				if( ismobile ) {
					
					tweenDelta = (currentDelta - ((currentDelta - newDelta)));
					if(currentDelta && currentDelta < 0 ) paralaxxed[i].style.transform = "translateY(" + tweenDelta + "px)";
					// if(currentDelta && currentDelta < 0 ) paralaxxed[i].style.webkitTransform = "translateY(" + tweenDelta + "px)";
					
				} else {
					
					var tweenDelta = (currentDelta - ((currentDelta - newDelta) * 0.08));
					// paralaxxed[i].style.transform = "translateY(" + tweenDelta + "px) translateZ(0)";
					// paralaxxed[i].style.webkitTransform = "translateY(" + tweenDelta + "px) translateZ(0)";
                    paralaxxed[i].style.transform = "translateY(" + tweenDelta + "px) translateZ(0)";
                    paralaxxed[i].style.webkitTransform = "translateY(" + tweenDelta + "px) translateZ(0)";
				}
				// paralaxxed[i].style.transform = "translate3d(0px," + tweenDelta + "px, 0px)";
                paralaxxed[i].dataset.currentDelta = tweenDelta;
            }
        }
		
		if(menuClosed) {
            window.requestAnimationFrame( scrollHandler );
        }
        // window.requestAnimationFrame( scrollHandler );
    }

    // init the thing
    function init() {
        var that = this;
        var paralaxxedsettings = {
             "multiplier" : "0.2",
             "direction": "up",
             "positionType": "absolute",
        };
        
        if(paralaxxed.length > 0) {
            parallaxate(that.paralaxxed, paralaxxedsettings);
        }
		
		scrollHandler();
        // sets the interval
    }
	
	pauseTween = function() {
		menuClosed = false;
        // console.log("pause parallax");
	}
	
	restartTween = function() {
		menuClosed = true;
        // console.log("restart parallax");
		scrollHandler();
	}
	
    init()
})();
// controls text effects

var EpicTextEffects = function() {
	
	// desktop elements
	var heroYear			= document.getElementById("heroYear");
	var hamburger			= document.getElementById("hamburger");
	var nonConf				= document.getElementById("nonConf");
	var requestinvite		= document.getElementById("requestinvite");
	var bigBadge			= document.getElementById("bigBadge");
	var bigUtah				= document.getElementById("bigUtah");
	var parkCityBig			= document.getElementById("parkCityBig");
	var eventDates			= document.getElementById("eventDates");
	var mtnReticle			= document.getElementById("mtnReticle");
	var twitterD			= document.getElementById("twitterD");
	var facebookD			= document.getElementById("facebookD");
	var instagramD			= document.getElementById("instagramD");
	var rLeft				= document.getElementById("rLeft");
	var rCenter				= document.getElementById("rCenter");
	var arrowCenter			= document.getElementById("arrowCenter");

	var pcSplit	 			= new SplitText(parkCityBig, {type: "words, chars"});
	var eventSplit	 		= new SplitText(eventDates, {type: "words, chars"});

	function init() {

		desktopElements = [heroYear, hamburger, nonConf, requestinvite, bigBadge, bigUtah, parkCityBig, eventDates, mtnReticle, twitterD, facebookD, instagramD, horzCredit, rLeft, rCenter, arrowCenter];

	}

	this.animateDeskTopHero = function() {

		// burger animate in
		TweenMax.from(hamburger, 1.3, {y: -1000, ease: Expo.easeOut});

		// request invite animate in
		TweenMax.from(requestinvite, 1.3, {y: -1000, delay: 0.15, ease: Expo.easeOut});

		// non-conference desktop animate in
		TweenMax.from(nonConf, 1.3, {y: -1000, delay: 0.3, ease: Expo.easeOut});
		// nonConf.style.display = "block";

		// Big hero desktop animate in
		TweenMax.from(heroYear, 1.3, {y: -1000, delay: 0.4, ease: Expo.easeOut});		

		// Big UTAH text animate in
		TweenMax.from(bigUtah, 1.3, {y: -1000, delay: 0.5, ease: Expo.easeOut});

		// Big Badge Animate In
		TweenMax.from(bigBadge, 1.3, {y: -1000, delay: 0.6, ease: Expo.easeOut});

		// Big Badge Animate In
		TweenMax.from(mtnReticle, 1.3, {y: 100, opacity: 0, delay: 0.7, ease: Expo.easeOut});

		// animate Park City Text
		TweenMax.staggerFrom(pcSplit.chars, 0.7, { 	opacity: 0, scale: 0, y: 50, delay: 0.8, rotationX: 180, transformOrigin: "0% 0% 0%", ease: Expo.easeOut }, 0.03, "+=0");

		// animate event dates in
		TweenMax.staggerFrom(eventSplit.chars, 0.7, { 	opacity: 0, scale: 0, y: 50, delay: 0.9, rotationX: 180, transformOrigin: "0% 0% 0%", ease: Expo.easeOut }, 0.01, "+=0");

		TweenMax.from(twitterD, 1.3, {y: 1000, delay: 1, ease: Expo.easeOut});
		TweenMax.from(facebookD, 1.3, {y: 1000, delay: 1.1, ease: Expo.easeOut});
		TweenMax.from(instagramD, 1.3, {y: 1000, delay: 1.2, ease: Expo.easeOut});
		TweenMax.from(horzCredit, 1.3, {y: 1000, delay: 1.25, ease: Expo.easeOut});
		TweenMax.from(rLeft, 1.3, {y: 1000, delay: 1.3, ease: Expo.easeOut});
		TweenMax.from(rCenter, 1.3, {y: 1000, delay: 1.35, ease: Expo.easeOut});
		TweenMax.from(arrowCenter, 1.3, {y: 1000, delay: 1.4, ease: Expo.easeOut});

		// loop through elements
		for(var i=0; i<desktopElements.length; i++) {

			desktopElements[i].style.visibility = "visible";

		}

	}


	init();


 }

 var epicText = new EpicTextEffects();
/**
 * video.js 0.0.2
 * 
 * This basically just checks to see if the video is visible and if it is, it plays the video.
 *
 */
 var videoController;

 ;(function() {
 	
 	videoController = this;

    // gets the offset thingy.
    function findOffsetOfElement(element){
        var offset = element.offsetTop;
        var currentParent = element.offsetParent;

        try {
            while(currentParent.offsetParent) {
                offset = offset + currentParent.offsetTop;
                currentParent = currentParent.offsetParent;
            }
        } catch(err) {
            console.log("ERROR with Video OFFSET");
        }
        
        return offset;
    }
    
    // HTML Stuff
    this.video = document.getElementById("video");
    // this.container = document.getElementById('container');
    // this.container = document.getElementsByTagName('body')[0];
    this.container = document.body;
    this.triggerSpot = findOffsetOfElement(this.video) - window.innerHeight;
    this.didNotPlay = true;
    this.videoLoaded = false;
    
    function setTriggerSpot(){
        setTimeout(function(){
            triggerSpot = (findOffsetOfElement(this.video) - window.innerHeight) * 0.66;
        }, 1000);
    }

    function init() {    
        window.onresize = setTriggerSpot;
        setTriggerSpot();
        
        // lifted from: http://stackoverflow.com/questions/19521667/disable-fullscreen-iphone-video
        video.addEventListener("contextmenu", function (e) { e.preventDefault(); e.stopPropagation(); }, false);
        if (video.hasAttribute("controls")) {
            video.removeAttribute("controls")   
        }
        
        if( !ismobile ) {
         
            window.addEventListener("scroll", function(event) {

                if(container.scrollTop > (triggerSpot / 2) && videoLoaded == false) {
                    video.src = "/viz/location-pc.mp4";
                    videoLoaded = true;
                    var notsupported = document.getElementsByClassName('video-not-supported')[0];
                    notsupported.parentNode.removeChild(notsupported);
                }
                
                if(visible(video)) {
                    if(didNotPlay == true) {
                        video.play();    
                        didNotPlay = false;
                    }
                } else {
                    if(didNotPlay == false) {
                        video.pause();
                    }
                    didNotPlay = true

                }
            });
        } else {
            video.parentNode.removeChild(video);
        }
    }
    init();
})();