// lifted from: http://stackoverflow.com/questions/4770025/how-to-disable-scrolling-temporarily
// Why? we need to disable scrolling in the perspective container, and the menu, but not the guillotine form.

// left: 37, up: 38, right: 39, down: 40,
// spacebar: 32, pageup: 33, pagedown: 34, end: 35, home: 36
var keys = {37: 1, 38: 1, 39: 1, 40: 1};

function preventDefault(e) {
  e = e || window.event;
  if (e.preventDefault)
      e.preventDefault();
  e.returnValue = false;  
}

function preventDefaultForScrollKeys(e) {
    if (keys[e.keyCode]) {
        preventDefault(e);
        return false;
    }
}

function disableScroll() {
  if (window.addEventListener) // older FF
      window.addEventListener('DOMMouseScroll', preventDefault, false);
  window.onwheel = preventDefault; // modern standard
  window.onmousewheel = document.onmousewheel = preventDefault; // older browsers, IE
  window.ontouchmove  = preventDefault; // mobile
  document.onkeydown  = preventDefaultForScrollKeys;
}

function enableScroll() {
    if (window.removeEventListener)
        window.removeEventListener('DOMMouseScroll', preventDefault, false);
    window.onmousewheel = document.onmousewheel = null; 
    window.onwheel = null; 
    window.ontouchmove = null;  
    document.onkeydown = null;  
}

// function disableScrollOnElement(element) {
//     if (window.addEventListener) // older FF
//         window.addEventListener('DOMMouseScroll', preventDefault, false);
//     element.onwheel = preventDefault; // modern standard
//     element.onmousewheel = element.onmousewheel = preventDefault; // older browsers, IE
//     element.ontouchmove  = preventDefault; // mobile
//     element.onkeydown  = preventDefaultForScrollKeys;
// }
// 
// function enableScrollOnElement(element) {
//     if (element.removeEventListener)
//         element.removeEventListener('DOMMouseScroll', preventDefault, false);
//     element.onmousewheel = element.onmousewheel = null; 
//     element.onwheel = null; 
//     element.ontouchmove = null;  
//     element.onkeydown = null;  
// }


function getPrefixData(){
    var t;
    var el = document.createElement('fake-element');

    var prefixData = {
        transition: {},
        transform: {}
    }

    var transforms = {
        "transform":"transform",
        "MozTransform":"-moz-transform",
        "WebkitTransform":"-webkit-transform"
    }

    var transitions = {
        "transition":"transition",
        "MozTransition":"-moz-transition",
        "WebkitTransition":"-webkit-transition"
    }

    var transitionEndEvents = {
        'transition':'transitionend',
        'MozTransition':'transitionend',
        'WebkitTransition':'webkitTransitionEnd'
    }

    for(t in transitions){
        if(el.style[t] !== undefined){
            prefixData.transition.jsProp = t;
            prefixData.transition.cssProp = transitions[t];
            break;
        }
    }

    for(t in transforms){
        if(el.style[t] !== undefined){
            prefixData.transform.jsProp = t;
            prefixData.transform.cssProp = transforms[t];
            break;
        }
    }

    for(t in transitionEndEvents){
        if(el.style[t] !== undefined){
        prefixData.transition.endEvent = transitionEndEvents[t];
        break;
        }
    }

    return prefixData;
}

var prefixData = getPrefixData();

function visiblePlus(element, partial, debug) {
    var bodyRect = document.body.getBoundingClientRect();
    var elementRect = element.getBoundingClientRect();
    var elementTop = elementRect.top - bodyRect.top;
    var elementBottom = elementTop + element.offsetHeight;

    var viewport = window;
    var viewTop = viewport.scrollY - viewport.innerHeight;
    var viewBottom = viewport.scrollY + viewport.innerHeight + viewport.innerHeight;

    var compareTop;
    var compareBottom;

    if(partial === true){
        compareTop = elementBottom;
        compareBottom = elementTop;
    }
    else {
        compareTop = elementTop;
        compareBottom = elementBottom;
    }
    // if (debug) {console.log("compareTop >= viewTop", compareTop + " >= " + viewTop);}
    // if (debug) {console.log("compareBottom <= viewBottom", compareBottom + " <= " + viewBottom);}
    var result = ((compareBottom <= viewBottom) && (compareTop >= viewTop));
    if (debug) {console.log("result", result);}
    return result;
};

function visible(element, debug) {
    var bodyRect = document.body.getBoundingClientRect();
    var elementRect = element.getBoundingClientRect();
    var elementTop = elementRect.top - bodyRect.top;
    var elementBottom = elementTop + element.offsetHeight;

    var viewport = window;
    var viewTop = viewport.scrollY - viewport.innerHeight;
    var viewBottom = viewport.scrollY + viewport.innerHeight;
    
    var compareTop;
    var compareBottom;

    compareTop = elementBottom;
    compareBottom = elementTop;
    
    var result = ((compareBottom <= viewBottom) && (compareTop >= viewTop));
    
    if(debug) {
        // console.log("viewBottom", viewBottom);
        // console.log("elementTop", elementTop);
        // if (debug) {console.log("compareTop >= viewTop", compareTop + " >= " + viewTop);}
        // if (debug) {console.log("compareBottom <= viewBottom", compareBottom + " <= " + viewBottom);}
        console.log("result", result);
    }

    
    
    return result;
};

// serialize Forms elements
_util = {
    // accepts a solid form element
    getFormJSON: function(element) {
        json = {}
        for ( var i = 0; i < element.children.length; i++ ) {
            if ( element.children[i].tagName == 'INPUT') {
                // store this value
                var name = element.children[i].name
                var value = element.children[i].value
                json[name] = value
            }
        }
        return json
    }
}
// heavily modified and custom 

var siteImageLoader;

;(function() {
        
    
    function setTransition(element, transition) {
        transition = transition.replace(/transform/g, prefixData.transform.cssProp);
        element.style[prefixData.transition.jsProp] = transition;
        element.clientHeight; // force redraw
    };
    

    
    
    function loadVisibleImages(e){
        var images = this.html.images;
        if (!images) return;

        for (var i=0; i<images.length; i++) {
            var image = images[i];

            if (visiblePlus(image, true)) {
                if(!image.loaded) {
                    image.style.opacity = "0";

                    var src = image.getAttribute("data-src");
                    
                    // console.log('src', src);

                    image.src = src;
                    image.onload = function() {
                        setTransition(this, "opacity 800ms cubic-bezier(0.260, 1.480, 0.470, 0.960)");
                        this.style.opacity = "1";
                        if(this.id == "snowboarderLoaded") {
                            console.log("FUCK YEA");
                            console.log("DO WE HAVE EPIC??? +++", epicText);
                            epicText.animateDeskTopHero();
                        }
                    }

                    image.loaded = true;
                }
            }
        }
    }

    function init() {
        
        this.html = {
            images: []
        };
        
        this.next = 0;
        
        // get all them images
        this.html.images = document.body.querySelectorAll("img[data-src]");
        var images = this.html.images;
        
        // Load all of the visible images in nested timeouts
        // loadNextImage();
        
        window.addEventListener('scroll', function(){
            clearTimeout(this.loaderTimeout);
            this.loaderTimeout = setTimeout(function(){
                loadVisibleImages();
            }, 300);
        });
        setTimeout(function(){
            loadVisibleImages();
        }, 1000);
    }

    init()
})();
/*!
 * classie - class helper functions
 * from bonzo https://github.com/ded/bonzo
 * 
 * classie.has( elem, 'my-class' ) -> true/false
 * classie.add( elem, 'my-new-class' )
 * classie.remove( elem, 'my-unwanted-class' )
 * classie.toggle( elem, 'my-class' )
 */

/*jshint browser: true, strict: true, undef: true */
/*global define: false */

;( function( window ) {

'use strict';

// class helper functions from bonzo https://github.com/ded/bonzo

function classReg( className ) {
  return new RegExp("(^|\\s+)" + className + "(\\s+|$)");
}

// classList support for class management
// altho to be fair, the api sucks because it won't accept multiple classes at once
var hasClass, addClass, removeClass;

if ( 'classList' in document.documentElement ) {
  hasClass = function( elem, c ) {
    return elem.classList.contains( c );
  };
  addClass = function( elem, c ) {
    elem.classList.add( c );
  };
  removeClass = function( elem, c ) {
    elem.classList.remove( c );
  };
}
else {
  hasClass = function( elem, c ) {
    return classReg( c ).test( elem.className );
  };
  addClass = function( elem, c ) {
    if ( !hasClass( elem, c ) ) {
      elem.className = elem.className + ' ' + c;
    }
  };
  removeClass = function( elem, c ) {
    elem.className = elem.className.replace( classReg( c ), ' ' );
  };
}

function toggleClass( elem, c ) {
  var fn = hasClass( elem, c ) ? removeClass : addClass;
  fn( elem, c );
}

var classie = {
  // full names
  hasClass: hasClass,
  addClass: addClass,
  removeClass: removeClass,
  toggleClass: toggleClass,
  // short names
  has: hasClass,
  add: addClass,
  remove: removeClass,
  toggle: toggleClass
};

// transport
if ( typeof define === 'function' && define.amd ) {
  // AMD
  define( classie );
} else {
  // browser global
  window.classie = classie;
}

})( window );

;(function() {
    
   document.flowtype = function(elements, options) {
       
       // checks for an array or makes one
       if( (elements instanceof Element) ){
            var array = [];
            var newElement = elements;
            array.push(newElement);
            elements = array;
       }
        
        // Totally lifted from this: http://stackoverflow.com/questions/11197247/javascript-equivalent-of-jquerys-extend-method
        function extend(){
            for(var i=1; i<arguments.length; i++)
                for(var key in arguments[i])
                    if(arguments[i].hasOwnProperty(key))
                        arguments[0][key] = arguments[i][key];
            return arguments[0];
        }
        
// Establish default settings/variables
// ====================================
        var settings = {
             maximum   : 9999,
             minimum   : 1,
             maxFont   : 9999,
             minFont   : 1,
             fontRatio : 10,
             topMarginRatio : 10,
         }
        combinedSettings = extend(settings, options),
        
// Do the magic math
// =================
      changes = function(el) {
        
        for (var i = 0; i < el.length; i++) { 

            if (window.innerWidth <= 650){
                var elw = el[i].parentNode.offsetWidth,
                   settings = el[i].flowTypeSettings,
                   width = elw > settings.maximum ? settings.maximum : elw < settings.minimum ? settings.minimum : elw,
                   fontBase = width * 0.65,
                   marginBase = (fontBase * 0.2),
                   topMarginBase = width / settings.fontRatio,
                   fontsize = fontBase > settings.maxFont ? settings.maxFont : fontBase < settings.minFont ? settings.minFont : fontBase;
            } else {
                var elw = el[i].parentNode.offsetWidth,
                   settings = el[i].flowTypeSettings,
                   width = elw > settings.maximum ? settings.maximum : elw < settings.minimum ? settings.minimum : elw,
                   fontBase = width / settings.fontRatio,
                   marginBase = (width * 1.05) - (width / settings.fontRatio),
                   topMarginBase = width / settings.fontRatio,
                   fontsize = fontBase > settings.maxFont ? settings.maxFont : fontBase < settings.minFont ? settings.minFont : fontBase;
            }

            el[i].style.fontSize = fontsize + 'px';
            el[i].style.marginLeft = marginBase + 'px';
            el[i].style.marginTop = "-" + topMarginBase + 'px';
         }
      }

// Make the magic visible
// ======================
      // Context for resize callback
          for (i = 0; i < elements.length; i++) {
              elements[i].flowTypeSettings = combinedSettings;
          }
      // Make Changes upon resize
          window.addEventListener('resize', function(){
              changes(elements);
          });
      // Set changes on load
          changes(elements);
   };
}());
/**
 * menutoggle.js v0.0.1
 */
 ;(function() {
     
     var clickevent = mobilecheck() ? 'touchstart' : 'click';
     
     function init() {
         var menuitems = document.getElementsByClassName( 'menu-toggle' );
         for (i = 0; i < menuitems.length; i++) {
             var inputThingy = menuitems[i].getElementsByTagName( 'a' )[0];
             inputThingy.addEventListener( clickevent, function( ev ) {
                 ev.stopPropagation()
                 ev.preventDefault()
                 
                 console.log('clicked', ev.srcElement.parentElement);
                //  var list = ev.srcElement.parentElement.getElementsByTagName('ul')[0];
                 console.log('clicked', ev.srcElement.parentElement);

                 classie.add(ev.srcElement.parentElement, 'open');
                //  classie.remove(ev.srcElement.parentElement, 'open');
            });
         }
     }
    init()
})()
