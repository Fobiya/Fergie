 var paymentform;
 
;(function() {
    
    paymentform = this;
    
    var clickevent = mobilecheck() ? 'touchstart' : 'click';
	
    // HTML Stuff
    this.button         = document.getElementById("purchasebutton");
    this.largeheader    = document.getElementById("largeheader");
    this.container      = document.getElementById('container');


    var height          = document.getElementById("height");
    var sizeM           = document.getElementById("size m");
    var sizeF           = document.getElementById("size f");
    var helmet          = document.getElementById("helmet");
    var jacket          = document.getElementById("jacket");
    var pants           = document.getElementById("pants");
    var gloves          = document.getElementById("gloves");
    var shirtM          = document.getElementById("tshirt m");
    var shirtF          = document.getElementById("tshirt f");
    var inputRoom       = document.getElementById("inputblock padded");
    var roomToggle      = document.getElementById("roommate");
    var inputfield      = document.getElementsByClassName('enter-pref')[0];


    // form element groups

    var lodgingGroup         = document.getElementsByName('lodging');
    var ticketGroup          = document.getElementsByName('tickets');
    var rentalGroup          = document.getElementsByName('rentals');
    var heightGroup          = document.getElementsByName('height');
    var mShoesGroup          = document.getElementsByName('shoe-size-male');
    var fShoesGroup          = document.getElementsByName('shoe-size-female');
    var helmetsGroup         = document.getElementsByName('helmet-size');
    var jacketsGroup         = document.getElementsByName('jacket-size');
    var pantsGroup           = document.getElementsByName('pants-size');
    var glovesGroup          = document.getElementsByName('glove-size');
    var lessonsGroup         = document.getElementsByName('lesson');
    var tshirtMGroup         = document.getElementsByName('tshirt-size-m');
    var tshirtFGroup         = document.getElementsByName('tshirt-size-f');

    // console.log("this is the HEIGHT group: ", heightGroup);

    self.submit = function(stripe_token, hash) {
        
        stripe_token = stripe_token || null;
        hash = hash || null;

        var name               = document.getElementsByClassName('nameinput')[0].value;
        var email              = document.getElementsByClassName('emailinput')[0].value;
        var twitter            = document.getElementsByClassName('twitterinput')[0].value;
        var facebook           = document.getElementsByClassName('facebookinput')[0].value;


        // radio values

        var lodgingValue       = getValue( lodgingGroup, "radio" );
        var ticketValue        = getValue( ticketGroup, "radio" );
        var rentalValue        = getValue( rentalGroup, "radio" );
        var lessonValue        = getValue( lessonsGroup, "radio" );

        
        // drop down selected values

        var heightValue        = getValue( heightGroup, "jump" );
        var mShoeValue         = getValue( mShoesGroup, "jump" );
        var fShoeValue         = getValue( fShoesGroup, "jump" );
        var helmetValue        = getValue( helmetsGroup, "jump" );
        var jacketValue        = getValue( jacketsGroup, "jump" );
        var pantValue          = getValue( pantsGroup, "jump" );
        var gloveValue         = getValue( glovesGroup, "jump" );
        var tshirtMValue       = getValue( tshirtMGroup, "jump" );
        var tshirtFValue       = getValue( tshirtFGroup, "jump" );

        var roommatePref;

        if(inputfield.value != "No Preference") {
            roommatePref = inputfield.value;
        } else {
            roommatePref = "No Preference";
        }

        var data = { "name": name, "email": email, "twitter": twitter, "facebook": facebook, "lodging-pref": lodgingValue,
                                    "need-tickets": ticketValue, "need-rentals": rentalValue, "height": heightValue, "male-shoe-size": mShoeValue, 
                                    "female-shoe-size": fShoeValue, "helment-size": helmetValue, "jacket-size": jacketValue, "pant-size": pantValue,
                                    "glove-size": gloveValue, "shirt-size-male": tshirtMValue, "shirt-size-female": tshirtFValue, "roommate-pref": roommatePref, "lesson": lessonValue, "stripe_token": stripe_token, "hash": hash
                                 };

        console.log("data", data);

        var msg;
        var headerTxt;

          reqwest({
              url: '/invite/payment',
              method: 'post',
              data: data,
              type: 'json',
              success: function(resp) {
                  // handle valid submission
                  console.log(resp);
              },
              error: function(resp) {
                  console.log(resp);
                  response = JSON.parse(resp.response)
              }
          });
//         successOut(headerTxt);
//         
//         reqwest({
//             url: '/signup',
//             method: 'post',
//             data: data,
//             type: 'json',
//             success: function(resp) {
//                 // handle valid submission
//                 console.log(resp);
//                 
//                 headerTxt = "STOKED!";
//                 successOut(headerTxt);
//                 
//             },
//             error: function(resp) {
//                 console.log(resp);
//                 // classie.remove(button, 'sent');
//                 response = JSON.parse(resp.response)
//                 if (response.error == "Email already submitted") {
//                     
//                     msg = "WE GOT YOUR APPLICATION";
//                     headerTxt = "RELAX";
// 
//                     // handle the already submitted error
//                 } else {
//                     // handle some other error
//                     headerTxt = "ERROR";
//                     msg = 'PLEASE TRY AGAIN';
// 
//                 }
// 
//                 setTimeout(function() { errorOut( msg, headerTxt ) }, 400);
//             }
//         });

    }
    
    function submitting(){
        
        button.value = "";

        if( classie.hasClass(button, 'error')) {

            classie.remove(button, 'error');

        } else {

            classie.add(button, 'sent');

            largeheader.style.opacity = 0;
        
        }
    }


    function getValue(obj, kind) {

        var objValue;

        if(kind == "radio") {

            for(var i=0; i<obj.length; i++) {

                if(obj[i].checked == true) {
                    objValue = obj[i].value;
                }

            }

        }

        else if(kind == "jump") {

            if(obj[0].value && obj[0].value != "") {
                objValue = obj[0].value;
            }

        }

        return objValue;
    }



    function errorOut(btnMsg, headerTxt){
        
        button.value = btnMsg;
        classie.add(button, 'error');

        largeheader.innerHTML = headerTxt;
        largeheader.style.opacity = 1;

    }
    
    function successOut(headerTxt){
        
        largeheader.innerHTML = headerTxt;
        largeheader.style.opacity = 1;

    }

    function selectOnChange(event) {

        var element = event.target;

        // toggle select drop downs for genders
        if(element == sizeF && classie.hasClass(sizeM, 'selected'))
            classie.remove(sizeM, 'selected');

        else if(element == sizeM && classie.hasClass(sizeF, 'selected'))
            classie.remove(sizeF, 'selected');            

        else if(element == shirtF && classie.hasClass(shirtM, 'selected'))
            classie.remove(shirtM, 'selected');
    
        else if(element == shirtM && classie.hasClass(shirtF, 'selected'))
            classie.remove(shirtF, 'selected');
        
        classie.add(element, 'selected');

    }


    function roommateClicked(event) {

        var element = event.target;

        if(element != roomToggle) {
            classie.remove(roomToggle, 'no-pref');
        }

        else if(element == roomToggle) {

            inputfield.value = "";
            classie.add(roomToggle, 'no-pref');
            classie.remove(inputRoom, 'filled');
             
        }   

    }

    function init() {

        height.addEventListener("change", selectOnChange);
        sizeM.addEventListener("change", selectOnChange);
        sizeF.addEventListener("change", selectOnChange);
        helmet.addEventListener("change", selectOnChange);
        jacket.addEventListener("change", selectOnChange);
        pants.addEventListener("change", selectOnChange);
        gloves.addEventListener("change", selectOnChange);
        shirtF.addEventListener("change", selectOnChange);
        shirtM.addEventListener("change", selectOnChange);

        inputRoom.addEventListener("click", roommateClicked);
        roomToggle.addEventListener("click", roommateClicked);

        button.addEventListener("click", function(event) {
            console.log('pressed')
            event.stopPropagation()
            event.preventDefault()
            classie.add(event.srcElement, 'submitting');
            self.submit();
        });

    }
    if (height != null) {
        init()
    }
    
})();
/**
 * form.js 0.0.1
 * handles the form submission and the ajax stuff.
 */
;(function() {
    
    var clickevent = mobilecheck() ? 'touchstart' : 'click';
	
    // HTML Stuff
    this.button         = document.getElementById("submitinvitebutton");
    this.largeheader    = document.getElementById("largeheader");
    this.container      = document.getElementById('container');
    
    function submit() {

        var  name      = document.getElementsByClassName('nameinput')[0].value;
        var  email     = document.getElementsByClassName('emailinput')[0].value;
        var  twitter   = document.getElementsByClassName('twitterinput')[0].value;
        var  message   = document.getElementsByClassName('messageinput')[0].value;

        var data = { "name": name, "email": email, "twitter": twitter, "message": message }
        console.log("data", data);        

        submitting();

        var msg;
        var headerTxt = "";

        successOut(headerTxt);
        
        reqwest({
            url: '/signup',
            method: 'post',
            data: data,
            type: 'json',
            success: function(resp) {
                // handle valid submission
                console.log(resp);
                
                headerTxt = "STOKED!";
                successOut(headerTxt);
                
            },
            error: function(resp) {
                console.log(resp);
                // classie.remove(button, 'sent');
                response = JSON.parse(resp.response)
                if (response.error == "Email already submitted") {
                    
                    msg = "WE GOT YOUR APPLICATION";
                    headerTxt = "RELAX";

                    // handle the already submitted error
                } else {
                    // handle some other error
                    headerTxt = "ERROR";
                    msg = 'PLEASE TRY AGAIN';

                }

                setTimeout(function() { errorOut( msg, headerTxt ) }, 400);
            }
        });
    }
    

    function submitting(){
        
        button.value = "";

        if( classie.hasClass(button, 'error')) {

            classie.remove(button, 'error');

        } else {

            classie.add(button, 'sent');

            largeheader.style.opacity = 0;
        
        }
    }

    function errorOut(btnMsg, headerTxt){
        
        button.value = btnMsg;
        classie.add(button, 'error');

        largeheader.innerHTML = headerTxt;
        largeheader.style.opacity = 1;

    }
    
    function successOut(headerTxt){
        
        largeheader.innerHTML = headerTxt;
        largeheader.style.opacity = 1;

    }
    
    
    function init() {
        
        if (button !== null ) {
            button.addEventListener("click", function(event) {
                console.log('pressed')
                event.stopPropagation()
                event.preventDefault()
                classie.add(event.srcElement, 'submitting');
                submit();
            });            
        }
        

    }
    
    init()
})();
